class Main{
    function void main(){
        var int i;
        var int j;
        var int temp;
        var Array arr;
        var int length;

        let length = Keyboard.readInt("Enter no of elements:");
        let arr = Array.new(length);

        let i = 0;

        while(i < length){
            let arr[i] = Keyboard.readInt("");
            let i = i + 1;
        }

        let i = 0;

        while(i < length){
            let j = i + 1;
            while(j < length){
                if (arr[i] > arr[j]){
                    let temp = arr[i];
                    let arr[i] = arr[j];
                    let arr[j] = temp;
                }
                let j = j + 1;
            }
            let i = i + 1;
        }

        let i = 0;
        while(i < length){
            do Output.printInt(arr[i]);
            do Output.printString(" ");
            let i = i + 1;
        }
        return; 
    }
}